{
  "name": "single-page-app",
  "version": "0.1.0",
  "homepage": "https://WernerO19.github.io/single-page-app",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-spring/web": "^9.7.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.4",
    "postcss-cli": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.8.2",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "react-spring": "^9.7.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build:css": "postcss src/index.css -o src/tailwind.css",
    "watch:css": "postcss src/index.css -o src/tailwind.css -w",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start && build:css",
    "build": "react-scripts build && build:css",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^5.0.0",
    "tailwindcss": "^3.2.7"
  }
}

